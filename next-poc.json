{
    "family": "next-poc",
    "executionRoleArn": "arn:aws:iam::496645848084:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "next-poc",
            "image": "496645848084.dkr.ecr.us-east-1.amazonaws.com/next-poc:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
